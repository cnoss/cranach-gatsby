{
  "name": "cranach-gatsbyjs",
  "private": true,
  "description": "Cranach graphics project based on GatsbyJS",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:with-prefix": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "dev-network": "gatsby develop -H $HOSTNAME -p 8000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext js --ext jsx src/",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy-live": "gatsby build --prefix-paths && cp deploy-helper/htaccess public/.htaccess && node deploy-helper/zip-public.js",
    "create": "node scripts/create.js"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^5.3.21",
    "@storybook/addons": "^5.3.21",
    "@storybook/react": "^5.3.21",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "babel-loader": "^8.1.0",
    "gatsby": "^2.24.69",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-react-helmet": "^3.3.13",
    "gatsby-plugin-sass": "^2.3.16",
    "gatsby-plugin-transition-link": "^1.18.0",
    "gatsby-source-filesystem": "^2.3.33",
    "gatsby-transformer-json": "^2.4.14",
    "gsap": "^3.5.1",
    "i18next": "^19.8.2",
    "openseadragon": "^2.4.2",
    "react": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.7.3",
    "sass": "^1.26.12"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.36",
    "child_process": "^1.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react-hooks": "^2.5.1",
    "gatsby-plugin-eslint": "^2.0.8",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react-dom": "^16.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cnoss/cranach-gatsby"
  },
  "bugs": {
    "url": "https://github.com/cnoss/cranach-gatsby/issues"
  }
}
