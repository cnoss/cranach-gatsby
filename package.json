{
  "name": "cranach-gatsbyjs",
  "private": true,
  "description": "Cranach graphics project based on GatsbyJS",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext js --ext jsx src/",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "create": "node scripts/create.js"
  },
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-notes": "^5.2.6",
    "@storybook/addon-viewport": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "babel-loader": "^8.0.6",
    "gatsby": "^2.18.18",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sass": "^2.1.24",
    "gatsby-source-filesystem": "^2.1.39",
    "gatsby-transformer-json": "^2.2.20",
    "i18next": "^19.0.3",
    "i18next-xhr-backend": "^3.2.2",
    "openseadragon": "^2.4.1",
    "react": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.2.7",
    "sass": "^1.23.7"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.23",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gh-pages": "^2.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react-dom": "^16.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cnoss/cranach-gatsby"
  },
  "bugs": {
    "url": "https://github.com/cnoss/cranach-gatsby/issues"
  }
}
